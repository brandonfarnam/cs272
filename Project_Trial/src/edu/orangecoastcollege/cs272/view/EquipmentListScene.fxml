<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="280.0" prefWidth="381.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.orangecoastcollege.cs272.view.EquipmentListScene">
   <bottom>
      <HBox prefHeight="66.0" prefWidth="279.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#addEquipmentToInventory" prefHeight="27.0" prefWidth="194.0" text="Add Equipment to Inventory" />
            <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#viewInventory" prefHeight="27.0" prefWidth="211.0" text="View Equipments">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#Continue" prefHeight="27.0" prefWidth="81.0" text="Continue" />
               </children>
            </HBox>
         </children>
      </HBox>
   </bottom>
   <right>
      <ListView fx:id="allEquipmentLV" prefHeight="214.0" prefWidth="384.0" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
